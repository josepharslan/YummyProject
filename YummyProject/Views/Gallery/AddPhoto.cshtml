@model YummyProject.Models.PhotoGallery

@{
    ViewBag.Title = "AddPhoto";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<form method="post" enctype="multipart/form-data">
    @Html.HiddenFor(x => x.PhotoGalleryId)

    <div class="mb-3">
        <label>Yeni Resim Yükle</label>
        <input type="file" name="ImageFile" class="form-control" />
    </div>

    <button class="btn btn-success">Resim Ekle</button>
</form>



